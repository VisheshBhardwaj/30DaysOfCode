import java.util.Scanner;

public class Day9 {
		
	public int T;
	public int factorial(int N) {
		
		if(N>0) {
			return N*factorial(N-1);
		}
		else {
			return N=1;
		}
	
		
	}
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		Day9 c = new Day9();
		int x=sc.nextInt();
		
		int y=c.factorial(x);
		System.out.println(y);
	}

}
