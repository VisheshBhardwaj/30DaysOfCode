import java.util.Scanner;

public class Solution {

		public static void main(String[] args) {
			Scanner scan = new Scanner(System.in);
			
			int n = scan.nextInt();
			
			 if(n%2==0) {
				 if(2<=n&&n<=5)
         {
						System.out.println("Not Weird");
           }
				 if(6<=n&&n<=20) {
					 System.out.println("Weird");
				   }
				 if(n>20) {
					 System.out.println("Not Weird");
				   }
			  }
		
			 else 
       {
				 System.out.println("Weird");
			 }
			}
		}
